{:deps {:aliases [:dev :test]},
 :builds
 {:dev
  {:target :node-library,
   :exports {:handler atomist.main/handler},
   :output-to "index.js"},
  :release
  {:target :node-script
   :main atomist.main/handler
   :output-to "index.js"
   :exports {:handler atomist.main/handler}
   :compiler-options {:optimizations :simple}},
  :test
  {:target :node-test,
   :output-to "tests.js", 
   :ns-regexp "-t$", 
   :autorun true}}}
